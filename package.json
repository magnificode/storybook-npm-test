{
  "name": "@houseofgiants/sbds-test",
  "version": "0.1.0",
  "main": "dist/index.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.3",
    "typescript": "^4.2.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "release": "auto shipit --base-branch=main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@storybook/addon-actions": "^6.2.5",
    "@storybook/addon-essentials": "^6.2.5",
    "@storybook/addon-links": "^6.2.5",
    "@storybook/node-logger": "^6.2.5",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.5",
    "auto": "^10.25.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.2.1",
    "storybook-addon-jsx": "^7.3.7"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/house-of-giants/storybook-npm-test.git"
  },
  "author": "Dominic Magnifico",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/house-of-giants/storybook-npm-test/issues"
  },
  "homepage": "https://github.com/house-of-giants/storybook-npm-test#readme"
}
